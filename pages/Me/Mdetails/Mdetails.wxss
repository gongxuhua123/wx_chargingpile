page {
  background-color: #fff;
}

.myrate{
  height: 25rpx !important;
  overflow: hidden;
}

.mdet {
  width: 87.333%;
  padding: 0 46rpx;
  position: absolute;
  z-index: 20;
  background-color: #fff;
  border-radius: 22rpx 22rpx 0 0;
  margin-top: -15rpx;
}
.mdet1{
  width: 100%;
  height: 128rpx;
  line-height: 128rpx;
    color: #2D3142;
  font-weight: bold;
  border-bottom:1px solid rgba(36,52,90,0.1);
}
.mdet1 image:first-child{
  max-width: 88rpx;
  max-height: 64rpx;
  float: left;
  margin: 30rpx 30rpx 0 0;
}
.mdet1 text:nth-child(2){
  font-size: 32rpx;
  color: #2D3142;
  font-weight: bold;
  float: left;
}
.mdet1-1{
  width: 70rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  background-color: #91D5FF;
  font-size: 22rpx;
  color: #1890FF;
  float: left;
  margin: 48rpx 0 0 20rpx;
}
.slot{
  font-size: 36rpx;
  float: right;
}
.rowlevel1{
  max-width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid rgba(36,52,90,0.1);
}
.rowlevel1 text:first-child{
  color: #2D3142;font-size: 28rpx;
  float: left;
}
.rowlevel1 text:nth-child(2){
  color: #4B5F7C;font-size: 32rpx;
  float: right;
}